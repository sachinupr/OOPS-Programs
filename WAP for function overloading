

// WAP for function oveloading

#include<bits/stdc++.h>
using namespace std;
class hero
{
    public:
    int fun (int x, int y)
    {
        return x+y;
    }
    int fun (int x)
    {
        return x*x;
    }
    int fun (int x,int y, int z)
    {
        return x*x+y*y+z*z;
    }
};
int main()
{
    hero obj;
    cout<<"output is : "<<obj.fun(5,6)<<endl;
    cout<<"output is : "<<obj.fun(4)<<endl;
    cout<<"output is : "<<obj.fun(8,9,1);
    return 0;
}
